        "redux-devtools-extension": "2.13.5",
        "rimraf": "2.6.2"
    },
    "proxy": {
        "/socket.io": {
            "target": "ws://localhost:3100",
            "ws": true
        },
        "/api": {
            "target": "http://localhost:3100"
        }
